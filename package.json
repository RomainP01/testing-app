{
  "name": "testing-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack-dev-server --mode=development",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack --mode=production --extensions .js,.jsx,.ts,.tsx --devtool source-map",
    "postbuild": "cp ./public/index.html ./dist/index.html",
    "preview": "serve --no-clipboard --single --listen 8080 dist",
    "lint": "eslint --ignore-path .gitignore ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --ignore-path .gitignore . --fix --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json)\"",
    "format:fix": "npm run format -- --write",
    "format:check": "npm run format -- --check",
    "types:check": "tsc",
    "check": "npm run types:check && npm run format:check && npm run lint && npm run build",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/runtime": "^7.7.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.3",
    "css-loader": "^3.2.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "file-loader": "^5.0.2",
    "husky": "^8.0.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.8.1",
    "serve": "^11.2.0",
    "style-loader": "^1.0.0",
    "typescript": "^5.0.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "6.2.1"
  }
}
